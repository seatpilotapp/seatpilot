# SeatPilot Release Toolkit environment configuration
# Copy this file to `.release.env` and reemplaza con tus valores reales.

export PR_NUMBER=4
export RELEASE_TAG=v1.0.0-prod-r1
export COMPOSE=docker-compose.metrics.local.yml

# Supabase database URL con sslmode=require
export SUPABASE_DB_URL="postgresql://postgres:password@db.your-project.supabase.co:5432/postgres?sslmode=require"

# Metrics API para seeds/demo
export METRICS_API_BASE_URL="http://localhost:8080"
export METRICS_API_KEY="dev-metrics-key"

# Opcional: overrides de seeds
# export SEED_TENANT=demo
# export SEED_CHANNEL=kiosk
